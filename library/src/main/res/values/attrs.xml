<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RangeCalendarView">
        <attr name="rangeCalendar_selectionColor" format="color"/>
        <attr name="rangeCalendar_dayNumberTextSize" format="dimension"/>
        <attr name="rangeCalendar_inMonthDayNumberColor" format="color"/>
        <attr name="rangeCalendar_outMonthDayNumberColor" format="color"/>
        <attr name="rangeCalendar_disabledDayNumberColor" format="color"/>
        <attr name="rangeCalendar_todayColor" format="color"/>
        <attr name="rangeCalendar_weekdayColor" format="color"/>
        <attr name="rangeCalendar_weekdayTextSize" format="dimension"/>
        <attr name="rangeCalendar_hoverColor" format="color"/>
        <attr name="rangeCalendar_hoverOnSelectionColor" format="color"/>
        <attr name="rangeCalendar_cellSize" format="dimension"/>
        <attr name="rangeCalendar_weekdayType" format="enum">
            <enum name="shortName" value="0"/>
            <enum name="narrowName" value="1"/>
        </attr>
        <attr name="rangeCalendar_clickOnCellSelectionBehavior" format="enum">
            <enum name="none" value="0"/>
            <enum name="clear" value="1"/>
        </attr>
        <attr name="rangeCalendar_cellRoundRadius" format="dimension"/>
        <attr name="rangeCalendar_vibrateOnSelectingCustomRange" format="boolean"/>
        <attr name="rangeCalendar_selectionFillGradientBoundsType" format="enum">
            <enum name="shape" value="0"/>
            <enum name="grid" value="1"/>
        </attr>
    </declare-styleable>

</resources>