<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RangeCalendarView">
        <attr name="rangeCalendar_selectionColor" format="color"/>
        <attr name="rangeCalendar_dayNumberTextSize" format="dimension"/>
        <attr name="rangeCalendar_inMonthDayNumberColor" format="color"/>
        <attr name="rangeCalendar_outMonthDayNumberColor" format="color"/>
        <attr name="rangeCalendar_disabledDayNumberColor" format="color"/>
        <attr name="rangeCalendar_todayColor" format="color"/>
        <attr name="rangeCalendar_weekdayColor" format="color"/>
        <attr name="rangeCalendar_weekdayTextSize" format="dimension"/>
        <attr name="rangeCalendar_hoverAlpha" format="float"/>
        <attr name="rangeCalendar_cellSize" format="dimension"/>
        <attr name="rangeCalendar_cellWidth" format="dimension"/>
        <attr name="rangeCalendar_cellHeight" format="dimension"/>
        <attr name="rangeCalendar_weekdayType" format="enum">
            <enum name="shortName" value="0"/>
            <enum name="narrowName" value="1"/>
        </attr>
        <attr name="rangeCalendar_weekdays" format="reference"/>
        <attr name="rangeCalendar_clickOnCellSelectionBehavior" format="enum">
            <enum name="none" value="0"/>
            <enum name="clear" value="1"/>
        </attr>
        <attr name="rangeCalendar_cellRoundRadius" format="dimension"/>
        <attr name="rangeCalendar_vibrateOnSelectingCustomRange" format="boolean"/>
        <attr name="rangeCalendar_selectionFillGradientBoundsType" format="enum">
            <enum name="shape" value="0"/>
            <enum name="grid" value="1"/>
        </attr>
        <attr name="rangeCalendar_cellAnimationType" format="enum">
            <enum name="alpha" value="0"/>
            <enum name="bubble" value="1"/>
        </attr>
        <attr name="rangeCalendar_showAdjacentMonths" format="boolean"/>
        <attr name="rangeCalendar_isSelectionAnimatedByDefault" format="boolean"/>
        <attr name="rangeCalendar_isHoverAnimationEnabled" format="boolean"/>
        <attr name="rangeCalendar_infoPattern" format="string"/>
        <attr name="rangeCalendar_useBestPatternForInfoPattern" format="boolean"/>
        <attr name="rangeCalendar_infoTextSize" format="dimension"/>
        <attr name="rangeCalendar_firstDayOfWeek" format="enum">
            <enum name="monday" value="0"/>
            <enum name="tuesday" value="1"/>
            <enum name="wednesday" value="2"/>
            <enum name="thursday" value="3"/>
            <enum name="friday" value="4"/>
            <enum name="saturday" value="5"/>
            <enum name="sunday" value="6"/>
        </attr>
    </declare-styleable>

</resources>